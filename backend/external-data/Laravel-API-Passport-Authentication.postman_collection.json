	"item": [
		{
			"name": "Registration",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var response = pm.response.json()\r",
							"var success = response.success;\r",
							"if(success === true){\r",
							"    var token = response.data.token;\r",
							"    pm.environment.set('token', token)\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "name",
							"value": "Rabiul Hasan",
							"type": "text"
						},
						{
							"key": "email",
							"value": "rabiul.fci@gmail.com",
							"type": "text"
						},
						{
							"key": "password",
							"value": "123456",
							"type": "text"
						},
						{
							"key": "confirm_password",
							"value": "123456",
							"type": "text"
						}
					]
				},
				"url": {
